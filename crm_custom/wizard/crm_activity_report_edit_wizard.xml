<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="crm_activity_report_edit_wizard_view_form" model="ir.ui.view">
        <field name="name">crm.activity.report.edit.wizard.view.form</field>
        <field name="model">activity.report.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group >
                        <group>
                            <field name="name" readonly="0"/>
                            <field name="date" readonly="0"/>
                            <field  name ="lead_id"/>
                            <field name="date_conversion" readonly="0" />
                            <field name="date_closed" readonly="0" />
                            
                        </group>


                        <group>
                            <field name="lead_create_date" readonly="0"/>
                            <field name="date_deadline" readonly="0" />
                            <field name="author_id" readonly="0" />
                            
                        </group>
                             
                        
                        
                    </group>
                    <group>

                        <group>
                            <field name="user_id" readonly="0"/>
                            <field name="mail_activity_type_id" readonly="0" />
                            <field name="company_id" readonly="0" />
                            <field name="partner_id" readonly="0" />
                            <field name="active" readonly="0" />
                         
                            
                        </group>
                     
                        <group>
                            <field name="team_id" readonly="0"/>
                            <field name="body" readonly="0" />
                            <field name="subtype_id" readonly="0" />
                            <field name="country_id" readonly="0" />
                            <field name="stage_id" readonly="0" />
                            <field name="lead_type" readonly="0" />
                            <field name="subtype_id" readonly="0" />
                          
                            
                            
                            
                        </group>



                    </group>
                   
                   
                </sheet>
                <footer>
                    <button name="action_create_lead_and_mailmessage"
                                string="save"
                                type="object"
                                class="btn-primary"
                                 />            

                </footer>
           

                

             
        
            </form>
        </field>
    </record>

    
   

    



    
    

    

</odoo>
