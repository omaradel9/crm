<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="sale_order_line_import_wizard_view_form" model="ir.ui.view">
        <field name="name">sale.order.line.import.wizard.view.form</field>
        <field name="model">sale.order.lines.import.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <group>
                            <button string="Download Empty File" name="action_download_sale_order_line_excel_doc" type="object" class="oe_highlight"/>
                            <button name="download_order_lines" string="Download Order lines" type="object" class="oe_highlight" 
                            invisible="1"
                            />
                        </group>
                        <group>
                            <field name="order_lines_file" />
                            <field name="import_lines_types" />
                        </group>
                     
                        
                    </group>
               
                   
                   
                </sheet>
                <footer>
                    <button name="action_import_sale_order_line"
                                string="Import"
                                type="object"
                                class="btn-primary"
                                 />            

                </footer>
           

                

             
        
            </form>
        </field>
    </record>

    
   

    



    
    

    

</odoo>