<odoo>
    <record id="set_payment_note_wizard_form" model="ir.ui.view">
        <field name="name">set.payment.note.form</field>
        <field name="model">set.payment.note</field>
        <field name="arch" type="xml">
            <form string="Add Payment Note">
                <group>
                 
                    <field name="payment_note"/>
                      
                    
                </group>
                <footer>
                    <button string="Ok" type="object" name="action_confirm" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_set_payment_note_wizard_form" model="ir.actions.act_window">
        <field name="name">set.payment.note.wizard.action</field>
        <field name="res_model">set.payment.note</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>